<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		 version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<servlet>
		<servlet-name>upload</servlet-name>
		<servlet-class>org.gwtjqueryfileupload.server.UploadServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>upload</servlet-name>
		<url-pattern>/gwtjqueryfileupload/upload</url-pattern>
	</servlet-mapping>

	<filter>
		<filter-name>gwtCacheControlFilter</filter-name>
		<filter-class>org.pcori.topicsgwt.server.GWTCacheControlFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>gwtCacheControlFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<welcome-file-list>
		<welcome-file>GWTJQueryFileUpload.html</welcome-file>
	</welcome-file-list>

</web-app>
